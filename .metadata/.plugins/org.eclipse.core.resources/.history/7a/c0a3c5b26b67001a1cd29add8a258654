package foo;

import javax.swing.plaf.synth.SynthSeparatorUI;

public class TicTacToe {

	public static void main(String[] args) {
		
		char [] [] gameBoard = {{' ', '|',' ','|',' '},
								{'-', '+','-','+','-'},
								{' ', '|',' ','|',' '},
								{'-', '+','-','+','-'},
								{' ', '|',' ','|',' '},};
		
		System.out.println(gameBoard);
		
		for(char[] row:gameBoard) {
			for(char c:row) {
				System.out.print(c);
			}
		}

	}

}
